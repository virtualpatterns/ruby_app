- self.content_for(:html, :page) do
  /
    #{__FILE__}
  %div{'data-role' => 'header'}
    = @back_button.render(:html)
    %h1
      Dialogs
  %div{'data-role' => 'content'}
    %div{'data-role' => 'controlgroup'}
      = @acknowledgement_link.render(:html) do |element|
        - element.content_for(:html, :link, 'Acknowledgement')
      = @confirmation_link.render(:html) do |element|
        - element.content_for(:html, :link, 'Confirmation')
      = @exception_link.render(:html) do |element|
        - element.content_for(:html, :link, 'Exception')
